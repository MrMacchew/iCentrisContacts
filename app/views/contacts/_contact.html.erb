<% @contacts.each do |contact| %>
  <tr id="<%= contact.id %>">
    <td><%= contact.name %></td>
    <td><%= contact.phone %></td>
    <td><%= contact.email %></td>
    <td><%= contact.address %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, method: :delete, remote: true %></td>
  </tr>
<% end %>