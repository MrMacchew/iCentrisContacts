<p id="notice"><%= notice %></p>

<h1><%= image_tag("icentrislogo.png") %> Address Book</h1>

<table id='contacts'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <%= render 'contact' %>
  </tbody>
</table>

<br>
<button data-toggle="collapse" data-target="#newContact">New Contact</button>

<div id="newContact" class="collapse well">
				<%= render 'form' %>
</div>
